{
  "rewrites": [
    {
      "source": "/",
      "destination": "/public/index.html"
    },
    {
      "source": "/contact",
      "destination": "/public/contact.html"
    },
    {
      "source": "/thanks",
      "destination": "/public/thanks.html"
    },
    {
      "source": "/Asetts/:path*",
      "destination": "/public/Asetts/:path*"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}